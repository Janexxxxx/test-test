#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/bin/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/bin/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/stylelint@15.4.0_typescript@4.9.3/node_modules:/Users/xujing/Documents/xj/test arrange/test-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/bin/stylelint.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint@15.4.0_typescript@4.9.3/node_modules/stylelint/bin/stylelint.js" "$@"
fi
